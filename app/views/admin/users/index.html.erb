<div class="users-index">
  <h2 class="users-title">コース</h2>
  <div class="user-card">
    <% @users.each do |user|%>
      <div class="row">
        <%= link_to(admin_user_path(user), class: "users-link") do %>
        <span><%= image_tag avatar_url(user), class: "user-round-img" %></span>
        <p class="team-time no-text-decoration"><%= "#{time_ago_in_words(user.updated_at).upcase}前" %></p>
        <% end %>
      </div>
    <% end %>
  </div>
</div>